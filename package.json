{
  "name": "nasoo",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "dev": "next dev",
    "devTurbo": "next dev --turbo",
    "build": "next build",
    "start": "next start -p 3030",
    "lint": "next lint",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write .",
    "frontMaster": "pm2 describe front-master  | grep -e 'status.*online' && pm2 restart front-master || pm2 start --name front-master npm -- start",
    "server": "git pull origin main && yarn install && yarn build --debug && yarn frontMaster && yarn cron",
    "zero": "./zero.sh",
    "generateStatics": "node ./src/scripts/general",
    "icons": "node ./src/scripts/icons",
    "getCount": "node ./src/scripts/getCount",
    "removeErrorBuild": "node ./src/scripts/removeError",
    "cron": "pm2 describe cron | grep -e 'status.*online' && pm2 restart cron || pm2 start ./src/scripts/cronJob/main.js --name cron",
    "siteMaps": "node ./src/scripts/cronJob/sitemaps.js"
  },
